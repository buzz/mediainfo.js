{
  "name": "mediainfo.js",
  "version": "0.2.2",
  "description": "Emscripten port of MediaInfoLib displays information about video and audio files.",
  "author": "buzz",
  "repository": {
    "type": "git",
    "url": "https://github.com/buzz/mediainfo.js"
  },
  "bugs": "https://github.com/buzz/mediainfo.js/issues",
  "homepage": "https://github.com/buzz/mediainfo.js",
  "license": "BSD-2-Clause",
  "keywords": [
    "mediainfo",
    "emscripten",
    "video",
    "audio",
    "media"
  ],
  "files": [
    "LICENSE.txt",
    "README.md",
    "dist/",
    "src/"
  ],
  "scripts": {
    "build": "gulp",
    "clean": "rimraf dist build",
    "lint": "eslint .",
    "release": "commit-and-tag-version",
    "test": "jest",
    "type-check": "tsc --noEmit --project ."
  },
  "packageManager": "pnpm@6.32.4",
  "engines": {
    "node": ">=14.16"
  },
  "bin": {
    "mediainfo.js": "./dist/esm/cli.js"
  },
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "browser": "./dist/esm-bundle/index.min.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/umd/index.min.js",
  "jsdelivr": "./dist/umd/index.min.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/esm-bundle/index.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./MediaInfoModule.wasm": "./dist/MediaInfoModule.wasm",
    "./package.json": "./package.json"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "commit-and-tag-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "build",
        "scope": "deps",
        "section": "Upgrades"
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "docs",
        "hidden": true
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "perf",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      }
    ]
  },
  "dependencies": {
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@jest/globals": "^29.6.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-virtual": "^3.0.1",
    "@rollup/stream": "^3.0.0",
    "@types/decompress": "^4.2.4",
    "@types/emscripten": "^1.39.7",
    "@types/gulp": "^4.0.13",
    "@types/gulp-rename": "^2.0.2",
    "@types/gulp-sourcemaps": "^0.0.36",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/vinyl-buffer": "^1.0.1",
    "@types/vinyl-source-stream": "^0.0.31",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@xmldom/xmldom": "^0.8.10",
    "babel-jest": "^29.6.1",
    "babel-plugin-add-import-extension": "^1.6.0",
    "commit-and-tag-version": "^11.2.2",
    "decompress": "^4.2.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "jest-environment-node": "^29.6.1",
    "jest-matcher-utils": "^29.6.1",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.3",
    "typescript": "^5.1.6",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "xpath": "^0.0.34"
  },
  "pnpm": {
    "patchedDependencies": {
      "gulp-sourcemaps@3.0.0": "patches/gulp-sourcemaps@3.0.0.patch"
    }
  }
}
